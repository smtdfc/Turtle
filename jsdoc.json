{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./dist/docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  }
}